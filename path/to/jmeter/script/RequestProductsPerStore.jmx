          <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="SaveStoresInFile" enabled="true">
            <stringProp name="scriptLanguage">java</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">import org.apache.commons.lang.StringUtils;
import java.io.File;
import java.io.FileOutputStream;
import java.io.PrintStream;

String storesInColumn = vars.get(&quot;storesInColumn&quot;);
int C0maxStores = Integer.parseInt(vars.get(&quot;C0maxStores&quot;));
//log.info(&quot;#############2 getting x stores: &quot; + C0MaxStores.toString() );

String maxStoresInColumn = &quot;&quot;;
String[] lines = storesInColumn.split(&quot;\n&quot;);

for(int i=0; i &lt; C0maxStores &amp;&amp; i &lt; lines.length ; i++) {
	maxStoresInColumn = maxStoresInColumn + lines[i] + &quot;\n&quot;;
	//Log.info (&quot;\n\n&quot; + maxStoresInColumn);
}

FileWriter fstream = null;
BufferedWriter out = null;

fstream = new FileWriter(vars.get(&quot;data_dir&quot;)+&quot;/&quot;+vars.get(&quot;storesFile&quot;), false);
out = new BufferedWriter(fstream);

out.write(maxStoresInColumn);
out.write(System.getProperty(&quot;line.separator&quot;));

out.close();
fstream.close();
</stringProp>
